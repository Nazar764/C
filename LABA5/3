#include <stdio.h>

int main() {
    float sum = 0;
    float number;
    int count = 0;

    printf("Введіть послідовність з п'яти дійсних чисел:\n");

    // Введення п'яти чисел
    for (int i = 0; i < 5; ++i) {
        scanf("%f", &number);
        sum += number;
        ++count;
        printf("Сума: %.2f, Середнє: %.2f\n", sum, sum / count);
    }

    return 0;
}
